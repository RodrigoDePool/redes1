#!/bin/bash

#tshark -r traza.pcap -T fields -e eth.type -e vlan.etype -e ip.proto > tipos

awk 'BEGIN{ lineas_total=0; lineas_ip=0; lineas_udp=0; lineas_tcp=0; }
	{lineas_total = lineas_total + 1;
	 print "Van 100";
	 if( $1 == 0800 || $2 == 0800 ){
	  	lineas_ip = lineas_ip + 1;
    	print "Van 100";
    	
		if( $2 == 6 || $3 == 6 )
      	    lineas_tcp = lineas_tcp + 1;
    	else if( $2 == 17 || $3 == 17 )
      		lineas_udp = lineas_udp + 1;
   }
 } END{print "fin";}'tipos
